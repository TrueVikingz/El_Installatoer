<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@id/loginFragment">

    <fragment android:id="@+id/loginFragment" android:name="com.example.electriccontrol.LoginFragment"
              android:label="fragment_login" tools:layout="@layout/fragment_login">
        <action android:id="@+id/action_loginFragment_to_registerFragment2" app:destination="@id/registerFragment"/>
        <action android:id="@+id/action_loginFragment_to_mainFragment2" app:destination="@id/mainFragment"/>
    </fragment>
    <fragment android:id="@+id/mainFragment" android:name="com.example.electriccontrol.mainFragment"
              android:label="fragment_main" tools:layout="@layout/fragment_main">
        <action android:id="@+id/action_mainFragment_to_accountFragment" app:destination="@id/accountFragment"/>
        <action android:id="@+id/action_mainFragment_to_assignmentFragment" app:destination="@id/assignmentFragment"/>
    </fragment>
    <fragment android:id="@+id/registerFragment" android:name="com.example.electriccontrol.RegisterFragment"
              android:label="fragment_register" tools:layout="@layout/fragment_register"/>
    <fragment android:id="@+id/accountFragment" android:name="com.example.electriccontrol.AccountFragment"
              android:label="fragment_account" tools:layout="@layout/fragment_account"/>
    <fragment android:id="@+id/assignmentFragment" android:name="com.example.electriccontrol.AssignmentFragment"
              android:label="fragment_assignment" tools:layout="@layout/fragment_assignment"/>
</navigation>